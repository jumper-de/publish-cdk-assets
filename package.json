{
  "name": "@flit/publish-cdk-assets",
  "version": "1.1.0",
  "description": "A tool to publish CDK assets to S3 from manifest.json",
  "keywords": [
    "aws",
    "awscdk",
    "asset"
  ],
  "homepage": "https://github.com/jumper-de/publish-cdk-assets",
  "bugs": "https://github.com/jumper-de/publish-cdk-assets/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/jumper-de/publish-cdk-assets.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Luis Vierroth",
    "email": "luis@jumper.de",
    "url": "https://github.com/p-mercury"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "pca": "./dist/bin/publish-cdk-assets.js"
  },
  "files": [
    "./dist",
    "./README.md"
  ],
  "scripts": {
    "build": "npm run prepack",
    "prepack": "rm -rf ./dist && tsc -b"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.583.0",
    "@aws-sdk/client-sts": "^3.583.0",
    "@aws-sdk/credential-providers": "^3.583.0",
    "aws-cdk-lib": "^2.143.0",
    "yargs": "^17.7.2",
    "zip-a-folder": "3.1.6"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "@types/archiver": "^6.0.2",
    "@types/is-glob": "^4.0.4",
    "@types/node": "^20.12.12",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "typedocOptions": {
    "entryPoints": "./src/index.ts",
    "readme": "./TYPEDOC.md"
  },
  "stability": "stable"
}
